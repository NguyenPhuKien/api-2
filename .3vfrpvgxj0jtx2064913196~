     if (args[0] == "1") {
  var money = (await Currencies.getData(event.senderID)).money
  if (money >= 0) {
  axios.get('https://nguyenlienmanh.com/girl?apikey=basilvip').then(res => {
  var video = res.data.data;
  let callback = function () {
          api.sendMessage({
            body: ``,
            attachment: fs.createReadStream(__dirname + `/cache/boobs.jpg`)
          }, event.threadID, () => fs.unlinkSync(__dirname + `/cache/boobs.jpg`), event.messageID);
        };
        request(video).pipe(fs.createWriteStream(__dirname + `/cache/boobs.jpg`)).on("close", callback);
        Currencies.setData(event.senderID, options = {money: money - 0})
      })
  } else return api.sendMessage("Bạn cần 1000 đô",event.threadID,event.messageID);
          }
