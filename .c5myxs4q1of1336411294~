module.exports.onLoad = () => {
    const fs = require("fs-thêm");
    yêu cầu const = yêu cầu ("yêu cầu");
    const dirMaterial = __dirname + `/noprefix/`;
    if (!fs.existsSync(dirMaterial + "noprefix")) fs.mkdirSync(dirMaterial, { đệ quy: true });
    nếu (!fs.existsSync(dirMaterial + "mv.gif")) yêu cầu("https://i.imgur.com/Fy8ZscG.gif").pipe(fs.createWriteStream(dirMaterial + "mv.gif")) ;
}

